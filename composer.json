{
    "name": "awcodes/filament-installer",
    "description": "Filament Admin Panel application installer.",
    "keywords": [
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "nunomaduro/termwind": "^1.14",
        "symfony/console": "^4.0|^5.0|^6.0",
        "symfony/process": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/filament"
    ],
    "autoload": {
        "psr-4": {
            "Filament\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filament\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
